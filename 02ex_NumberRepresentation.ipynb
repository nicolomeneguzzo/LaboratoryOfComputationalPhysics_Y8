    "\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "print('valore corretto=', np.pi/2)\n",
    "\n",
    "def lim(n):\n",
    "    h = 2/n\n",
    "    x = np.linspace(-1+h/2,1-h/2,n) #per non perdere precisione in y, altrimenti 1 e x**2 sono simili\n",
    "    y = np.sqrt(1-x**2)\n",
    "    I = np.sum(h*y)\n",
    "    return 'valore computato=', I\n",
    "\n",
    "lim(100)"
